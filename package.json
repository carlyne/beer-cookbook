  "babel": {
    "presets": [
      "@babel/preset-env"
    ],
    "plugins": [
      "@babel/plugin-transform-runtime"
    ]
  },
  "dependencies": {
    "@kevinbalicot/custom-element": "^1.1.0",
    "dotenv": "^8.2.0",
    "node-sass": "^4.14.1",
    "yion": "^1.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.11.1",
    "@babel/plugin-transform-runtime": "^7.11.0",
